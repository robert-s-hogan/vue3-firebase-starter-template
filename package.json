{
  "name": "templates",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean-install": "rm -rf node_modules package-lock.json && npm install",
    "build": "npm run lint && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "npx cypress open --component --browser edge",
    "test:e2e": "npx cypress open --e2e --browser edge",
    "test:run": "npx cypress run --component --browser edge",
    "test:e2e:run": "npx cypress run --e2e --browser edge"
  },
  "dependencies": {
    "firebase": "^10.14.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.8",
    "@cypress/vite-dev-server": "^5.2.0",
    "@cypress/vue": "^6.0.1",
    "@eslint/js": "^9.13.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.15.0",
    "eslint": "^9.13.0",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-vue": "^9.29.0",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "sinon": "^19.0.2",
    "tailwindcss": "^3.4.13",
    "vite": "^5.4.8"
  }
}
